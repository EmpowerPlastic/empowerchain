{
  "name": "empower-marketplace",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "prepare": "cd ../.. && husky install frontend/marketplace/.husky",
    "build": "run-p type-check build-only",
    "build:test": "run-p type-check \"build-only -- --mode test\"",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@apollo/client": "3.8.10",
    "@empower-plastic/empowerjs": "0.0.20",
    "@keplr-wallet/types": "0.12.64",
    "@logto/vue": "2.2.1",
    "@segment/analytics-next": "1.63.0",
    "@vue/apollo-composable": "4.0.1",
    "@vue/apollo-option": "4.0.0",
    "@vuepic/vue-datepicker": "7.4.1",
    "daisyui": "4.6.0",
    "graphql": "16.8.1",
    "graphql-tag": "2.12.6",
    "jspdf": "2.5.1",
    "jspdf-autotable": "3.8.1",
    "qrcode": "1.5.3",
    "rollbar": "2.26.2",
    "vue": "3.4.15",
    "vue-awesome-paginate": "1.1.46",
    "vue-multiselect": "^3.0.0-beta.3",
    "vue-router": "4.2.5",
    "vue3-carousel": "0.3.1",
    "vue3-google-map": "0.19.0",
    "vue3-lottie": "^3.2.0",
    "vue3-toastify": "0.2.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.7.2",
    "@tsconfig/node20": "20.1.2",
    "@types/jsdom": "21.1.6",
    "@types/node": "20.11.10",
    "@types/qrcode": "1.5.5",
    "@vitejs/plugin-vue": "5.0.3",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "12.0.0",
    "@vue/test-utils": "2.4.4",
    "@vue/tsconfig": "0.5.1",
    "autoprefixer": "10.4.17",
    "eslint": "8.56.0",
    "eslint-plugin-vue": "9.20.1",
    "husky": "9.0.6",
    "jsdom": "24.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "tailwindcss": "3.4.1",
    "typescript": "~5.3.3",
    "vite": "5.0.12",
    "vitest": "1.2.2",
    "vue-tsc": "1.8.27"
  },
  "engines": {
    "npm": ">=10.2.3",
    "node": ">=20.10.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.9.6"
  }
}
