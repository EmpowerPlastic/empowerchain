{
  "name": "empower-marketplace",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "prepare": "cd ../.. && husky install frontend/marketplace/.husky",
    "build": "run-p type-check build-only",
    "build:test": "run-p type-check \"build-only -- --mode test\"",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@apollo/client": "^3.8.7",
    "@empower-plastic/empowerjs": "0.0.18",
    "@keplr-wallet/types": "^0.12.44",
    "@logto/vue": "^2.1.0",
    "@vue/apollo-composable": "^4.0.0-beta.5",
    "@vue/apollo-option": "^4.0.0-beta.9",
    "@vuepic/vue-datepicker": "^7.2.2",
    "daisyui": "^4.4.4",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.7.1",
    "qrcode": "^1.5.3",
    "rollbar": "^2.26.2",
    "vue": "^3.3.2",
    "vue-awesome-paginate": "^1.1.46",
    "vue-multiselect": "^3.0.0-beta.2",
    "vue-router": "^4.2.5",
    "vue3-carousel": "^0.3.1",
    "vue3-google-map": "^0.18.0",
    "vue3-toastify": "^0.1.14"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.0",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.0",
    "@types/qrcode": "^1.5.5",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.2",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.3.2",
    "vite": "^5.0.5",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  },
  "engines": {
    "npm": ">=10.2.3",
    "node": ">=20.10.0"
  }
}
